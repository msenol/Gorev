    {
      "name": "UUID",
      "pattern": "/[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}/gi"
    },
    {
      "name": "Git SHA",
      "pattern": "/\\b[0-9a-f]{7,40}\\b/g"
    },
    {
      "name": "Version",
      "pattern": "/v?\\d+\\.\\d+\\.\\d+(?:-[a-zA-Z0-9]+(?:\\.[a-zA-Z0-9]+)*)*/g"
